  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 职业类型分析\n",
    "print(\"职业类型分布\")\n",
    "print(df_base['occupation_type'].value_counts())\n",
    "\n",
    "print(\"\\n职业分布（Top 15）\")\n",
