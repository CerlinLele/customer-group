    "\n",
    "# 性别分布\n",
    "print(\"\\n性别分布\")\n",
    "print(df_base['gender'].value_counts())\n",
    "print(f\"\\n性别分布比例:\")\n",
    "print(df_base['gender'].value_counts(normalize=True).round(4))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 月收入分析\n",
    "print(\"月收入统计\")\n",
    "print(df_base['monthly_income'].describe())\n",
    "\n",
    "# 婚姻状态分析\n",
    "print(\"\\n婚姻状态分布\")\n",
    "print(df_base['marriage_status'].value_counts())\n",
    "\n",
    "# 城市等级分析\n",
    "print(\"\\n城市等级分布\")\n",
    "print(df_base['city_level'].value_counts())\n",
    "\n",
    "# 生命周期阶段分析\n",
    "print(\"\\n生命周期阶段分布\")\n",
