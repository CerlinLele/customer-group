  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 3. 客户行为和资产分析\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 资产水平分析\n",
    "print(\"资产水平分布\")\n",
    "print(df_behavior['asset_level'].value_counts())\n",
    "\n",
    "print(\"\\n总资产统计\")\n",
    "print(df_behavior['total_assets'].describe())\n",
    "\n",
    "# 各类资产统计\n",
    "asset_cols = ['deposit_balance', 'financial_balance', 'fund_balance', 'insurance_balance']\n",
    "print(\"\\n各类资产统计\")\n",
    "for col in asset_cols:\n",
    "    print(f\"\\n{col}:\")\n",
    "    print(f\"  均值: {df_behavior[col].mean():.2f}\")\n",
    "    print(f\"  中位数: {df_behavior[col].median():.2f}\")\n",
    "    print(f\"  标准差: {df_behavior[col].std():.2f}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 产品拥有情况\n",
    "print(\"产品持有标志统计\")\n",
    "product_flags = ['deposit_flag', 'financial_flag', 'fund_flag', 'insurance_flag']\n",
    "for flag in product_flags:\n",
    "    print(f\"\\n{flag}:\")\n",
    "    print(df_behavior[flag].value_counts())\n",
    "\n",
    "print(\"\\n产品数量统计\")\n",
    "print(df_behavior['product_count'].value_counts().sort_index())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 行为指标分析\n",
    "print(\"行为指标统计\")\n",
    "behavior_metrics = ['credit_card_monthly_expense', 'investment_monthly_count', \n",
    "                    'app_login_count', 'app_financial_view_time', 'app_product_compare_count']\n",
    "\n",
    "for metric in behavior_metrics:\n",
    "    print(f\"\\n{metric}:\")\n",
    "    print(f\"  均值: {df_behavior[metric].mean():.2f}\")\n",
    "    print(f\"  中位数: {df_behavior[metric].median():.2f}\")\n",
    "    print(f\"  最大值: {df_behavior[metric].max():.2f}\")\n",
    "    print(f\"  最小值: {df_behavior[metric].min():.2f}\")"
   ]
  },
  {
   "cell_type": {"metadata": {}, "source": [], "execution_count": null, "outputs": []},
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
