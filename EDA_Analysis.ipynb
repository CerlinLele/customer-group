    "## 6. 相关性分析\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 选择数值列进行相关性分析\n",
    "numeric_cols_base = df_base.select_dtypes(include=[np.number]).columns\n",
    "numeric_cols_behavior = df_behavior.select_dtypes(include=[np.number]).columns\n",
    "\n",
    "# 计算相关系数\n",
    "corr_base = df_base[numeric_cols_base].corr()\n",
    "corr_behavior = df_behavior[numeric_cols_behavior].corr()\n",
    "\n",
    "# 可视化行为数据的相关性\n",
    "fig, ax = plt.subplots(figsize=(12, 10))\n",
    "\n",
    "# 选择关键变量\n",
    "key_cols = ['total_assets', 'deposit_balance', 'financial_balance', 'fund_balance', \n",
    "            'insurance_balance', 'product_count', 'credit_card_monthly_expense',\n",
    "            'investment_monthly_count', 'app_login_count', 'app_financial_view_time']\n",
    "\n",
